proxy_redirect <%= scope.lookupvar('nginx::nx_proxy_redirect_real') %>;
client_max_body_size <%= scope.lookupvar('nginx::nx_client_max_body_size_real') %>;
client_body_buffer_size <%= scope.lookupvar('nginx::nx_client_body_buffer_size_real') %>;
proxy_connect_timeout <%= scope.lookupvar('nginx::nx_proxy_connect_timeout_real') %>;
proxy_send_timeout <%= scope.lookupvar('nginx::nx_proxy_send_timeout_real') %>;
proxy_read_timeout <%= scope.lookupvar('nginx::nx_proxy_read_timeout_real') %>;
proxy_buffers <%= scope.lookupvar('nginx::nx_proxy_buffers_real') %>;
